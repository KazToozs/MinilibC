
	global	my_memccpy
	section	.text

my_memccpy:
        mov     rax, rdi

loop:
        cmp     rcx, 0
        je      end
        dec     rcx
        mov     r8, [rsi]
	cmp	byte [rdx], r8b
	je	end
        mov     byte [rdi], r8b
        inc     rsi
        inc     rdi
        jmp     loop

end:
        ret
	
