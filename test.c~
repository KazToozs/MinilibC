
#include <string.h>
#include <stdio.h>

int	main(int ac, char **av)
{
  int c = av[2][0];

  printf("------ tests ------\n");
  printf("strchr: %s\n", strchr(av[1], c));
  printf("index: %s\n", index(av[1], c));
  printf("\n----- r tests -----\n");
  printf("strrchr: %s\n", strrchr(av[1], c));
  printf("rindex: %s\n", rindex(av[1], c));
}
